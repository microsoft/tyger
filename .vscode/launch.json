{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Control Plane",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/server/ControlPlane/bin/Debug/net8.0/tyger-server",
      "args": [],
      "cwd": "${workspaceFolder}/server/ControlPlane",
      "stopAtEntry": false,
      "console": "internalConsole",
      "justMyCode": true
    },
    {
      "name": "Data Plane",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/server/DataPlane/bin/Debug/net8.0/tyger-data-plane-server",
      "args": [],
      "cwd": "${workspaceFolder}/server/DataPlane",
      "stopAtEntry": false,
      "console": "internalConsole",
      "justMyCode": false
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach"
    },
    {
      "name": "CLI",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "cli/cmd/tyger",
      "cwd": "${workspaceFolder}",
      "args": [],
      "substitutePath": [
        {
          "from": "/workspaces/tyger-docker",
          "to": "/workspaces/tyger"
        }
      ]
    },
    {
      "name": "export",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "cli/cmd/buffer-copier",
      "cwd": "${workspaceFolder}",
      "args": [
        "export",
        "--db-host",
        "jostairs-tyger-intelligent-bardeen.postgres.database.azure.com",
        "--db-user",
        "SC-kr331@microsoft.com",
        "--source-storage-endpoint",
        "https://jostairseastus.blob.core.windows.net",
        "--destination-storage-endpoint",
        "https://jostairseastus.blob.core.windows.net",
        "--hash-ids"
      ]
    },
    {
      "name": "CLI Docker",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "cli/cmd/tyger",
      "cwd": "${workspaceFolder}",
      "args": ["login", "status", "--format", "json"],
      "env": {
        "TYGER_CACHE_FILE": "/home/vscode/.cache/tyger/.tyger-docker"
      },
      "substitutePath": [
        {
          "from": "/workspaces/tyger-docker",
          "to": "/workspaces/tyger"
        }
      ]
    },
    {
      "name": "buffer-sidecar",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "cli/cmd/buffer-sidecar",
      "cwd": "${workspaceFolder}",
      "args": [
        "relay",
        "read",
        "-b",
        "x",
        "-o",
        "i",
        "--listen",
        "unix:///tmp/xyz",
        "--tombstone",
        "/tmp/ssss"
      ],
      "substitutePath": [
        {
          "from": "/workspaces/tyger-docker",
          "to": "/workspaces/tyger"
        }
      ]
    }
  ]
}
